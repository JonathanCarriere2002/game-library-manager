<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include database in cloud backup -->
        <include domain="database" path="."/>
        <!-- Include shared preferences in cloud backup -->
        <include domain="sharedpref" path="."/>
    </cloud-backup>
    <device-transfer>
        <!-- Include database in device transfer -->
        <include domain="database" path="."/>
        <!-- Include general files in device transfer -->
        <include domain="file" path="."/>
        <!-- Include shared preferences in device transfer -->
        <include domain="sharedpref" path="."/>
    </device-transfer>
</data-extraction-rules>